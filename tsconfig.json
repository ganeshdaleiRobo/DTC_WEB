{
    "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "removeComments": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "preserveConstEnums": true,
    },
    "include": ["./src/index.d.ts"],
    "rules": { "quotemark": [true, "single"] },
    
    }